#include<iostream>
using namespace std;
void swap(int,int);
int main()
{
	int num1,num2;
	cout<<"enter the number 1 : ";
	cin>>num1;
	cout<<"enter the number 2 : ";
	cin>>num2;
	swap(num1,num2);
	return 0;
}
void swap(int num1,int num2)
{
	int temp;
	temp=num1;
	num1=num2;
	num2=temp;
	cout<< "the numbers after swaping is "<<num1<<"\t"<<num2;
}
